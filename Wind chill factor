#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
   int t, v;
    double wcf;

    // Read input values: temperature and wind velocity
    scanf("%d", &t);
    scanf("%d", &v);

    // Calculate Wind Chill Factor using the formula
    wcf = 35.74 + 0.6215 * t + (0.4275 * t - 35.75) * pow(v, 0.16);

    // Print the result rounded to 2 decimal places
    printf("%.2f\n", wcf);

    return 0;
}

  
