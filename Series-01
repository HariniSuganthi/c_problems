#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>
int main() {
    int n, i;
    scanf("%d", &n);  
    int term = 6;    
    int diff = 3;     
    for(i = 0; i < n; i++) {
        printf("%d", term);
        if(i != n - 1) {
            printf(" ");
        }
        term += diff;
        diff += 2;
    }

    return 0;
    
}
