#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int n;
    scanf("%d", &n);

    if(n < 0) {
        printf("Invalid Input\n");
    } else {
        long long factorial = 1;
        for(int i = 1; i <= n; i++) {
            factorial *= i;
        }
        printf("The Factorial of %d is %lld\n", n, factorial);
    }
    return 0;
}
