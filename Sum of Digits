#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int n;
    scanf("%d", &n);

    if(n >= 10 && n <= 99) {
        int firstDigit = n / 10;
        int secondDigit = n % 10;
        int sum = firstDigit + secondDigit;
        printf("Sum of Digit %d is %03d\n", n, sum); // 3-digit format
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}
